    <script>
        $(document).ready(function(){
            $(window).scroll(function(){
                if(this.scrollY > 5){
                    $('.navBar').addClass("sticky");
                }
                else{
                    $('.navBar').removeClass("sticky");
                }
            });
            // menu icon
            $('.menu_btn').click(function(){
                $('.navBar .menu').toggleClass("active");
                $('.menu_btn i').toggleClass("active");
            });

        });
    </script>

 