    <style>

        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@378;500;600;700&family=Ubuntu:wght@378;500;700&display=swap');
        *{
            margin: 0;
            padding:  0;
            box-sizing: border-box;
            text-decoration: none;
            
        }
        html{
            scroll-behavior: smooth;
            animation: forwards;
        }
        body{
            /* background-color: #000; */
            width: 100%;
        }
        header{
            width: 100%;
            height: 35px;
            background-color: rgb(124, 122, 123);
            position: static;
        }
        header ul{
            margin: 0;
            padding: 0;
            position: absolute;
            top: 10px;
            left: 50%;
            transform: translate(-50%, -50%);
            display: flex;
        }

        header ul li{
            list-style: none;
        }

        header ul li a{
            border: 1px solid #000;
            position: relative;
            display: block;
            width: 30px;
            height: 30px;
            text-align: center;
            line-height: 30px;
            color: #262626;
            overflow: hidden;
            margin: 15px 3px 0 3px;
            transition: .3s;
            border: none;
            text-decoration: none;
        }

        header ul li a .fa{
            transform: rotate(0);
        }
        header ul li a:before{
            content: '';
            position: absolute;
            bottom: -100px;
            left: -100px;
            width: 100%;
            height: 100%;
            background-color: rgb(36, 36, 33);
            transition: .2s;
        }

        .header ul li a:hover:before{
            top: 0;
            bottom: 0;
            left: 0;
        }
        header ul li a:hover{
            color: #fff;
        }


        .mainMenu{
            max-width: 100%;
            margin: 0 80px;
            padding: 0, 80px;
        }
 
        .navBar{
            position: fixed;
            font-family: 'Ubuntu', sans-serif;
            width: 100%;
            margin: auto;
            transition: all 0.2s ease-in-out;
            z-index: 998;
            /* border-bottom: 1px solid #000; */
            /* background: url("/img/laptop2.jpg"); */
            height: 60px;
        }

        .navBar.sticky{
            background:  rgb(124, 122, 123);
            position: relative;
            margin-top: -45px;
            /*crimson;*/
        }

        .navBar .mainMenu{
            display: flex;
            align-items: center;
            justify-content: space-between;
        }
        .navBar .logo img{
            height: 80px;
        }
        .navBar .menu li{
            list-style: none;
            display: inline-block;
        }
        .navBar .menu li a{
            color: #fff;
            font-size: 20px;
            font-weight: 100;
            margin-left: 20px;
            transition: color 0.3s ease;
        }

        .navBar.sticky .menu li a:hover{
            color: #000;
        }
        .navBar .menu li a:hover{
            color: rgb(163, 156, 157);
        }

        .navBar .menu_btn{
            color: #fff;
            font-size: 25px;
            display: none;
            cursor: pointer;
        }
.home{
            /* margin-top: 40px; */
            height: 550px;
            width: 100%;
            background: url("/img/pinkpet.jpg");
            background-position: center;
            background-repeat: no-repeat;
            
            position: relative;
        }

