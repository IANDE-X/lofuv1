	-webkit-transition: all 0.4s ;
    -o-transition: all 0.4s ;
    -moz-transition: all 0.4s ;
    transition: all 0.4s ;
}
.symbol-banner img {
	width: 100%;
	-webkit-transition: all 0.4s ;
    -o-transition: all 0.4s ;
    -moz-transition: all 0.4s ;
    transition: all 0.4s ;
}

.line-symbol-banner{
	position: absolute;
	height: 1px;
	background-color: #e52233;
	-webkit-transition: all 0.4s;
    -o-transition: all 0.4s;
    -moz-transition: all 0.4s;
    transition: all 0.4s;
}

#short-line-left{
	top:31px;
	right: 100%;
	width: 142px;
}
#long-line-left{
	top:39px;
